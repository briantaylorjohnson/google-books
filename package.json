{
  "name": "google-books",
  "version": "1.0.0",
  "main": "server.js",
  "engines": {
    "node": "8.11.3",
    "yarn": "1.x"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run start:prod || yarn run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn run client\"",
    "client": "cd client && yarn run start",
    "install": "cd client && yarn install",
    "build": "cd client && yarn run build",
    "heroku-postbuild": "yarn run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "description": "Georgia Tech Coding Bootcamp - Homework 16: Google Books Search",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briantaylorjohnson/google-books.git"
  },
  "author": "BTJ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/briantaylorjohnson/google-books/issues"
  },
  "homepage": "https://github.com/briantaylorjohnson/google-books#readme"
}
